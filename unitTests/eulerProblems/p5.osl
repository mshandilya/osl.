def i32 gcd(i32 a, i32 b)
{
    var i32 temp;
    while b ~= 0
    {
        temp := b;
        b := a % b;
        a := temp;
    }
    return a;
}
def i32 lcm(i32 a, i32 b) 
{
    return (a * b) / gcd(a, b);
}
def i32 F(i32 n, i32 i) {
    while i > 1 
    {
        n := lcm(n, i - 1);
        i := i - 1;
    }
    return n;
}
log F(1, 20);