<Prog> ::= <Decl> | <Decl> <Prog>
<Decl> ::= <VarDecl> | <Stmt> | <FunDecl>
<FunCall> ::= IDEN LPAREN <Params> RPAREN
<FunDecl> ::= FUN <Type> IDEN LPAREN <Params> RPAREN <Block> | FUN <Type> IDEN LPAREN RPAREN <Block>
<Params> ::= <Param> | <Param> COMMA <Params>
<Param> ::= <Type> IDEN
<Stmt> ::= <ExpStmt> | <Block> | <IfStmt> | <LogStmt> | <RetStmt>
<Block> ::= LBRACE RBRACE | LBRACE <Prog> RBRACE
<RetStmt> ::= RETURN <ExpStmt>
<IfStmt> ::= IF <Val> <Block> | IF <Val> <Block> <ElseStmt> | IF <Val> <Block> <ElifStmts> | IF <Val> <Block> <ElifStmts> <ElseStmt>
<ElseStmt> ::= ELSE <Block>
<ElifStmts> ::= <ElifStmt> | <ElifStmt> <ElifStmts>
<ElifStmt> ::= ELIF <Val> <Block>
<LogStmt> ::= LOG <Val> SEMC
<ExpStmt> ::= <Val> SEMC
<VarDecl> ::= <Mutable> | <Immutable>
<Mutable> ::= VAR <Type> IDEN SEMC | VAR <Type> <Assn> SEMC
<Immutable> ::= CONST <Type> <Assn> SEMC
<Val> ::= <Exp> | <Assn> | NULL
<Assn> ::= IDEN ASSIGN <Val>
<Exp> ::= <UnAmb> | <Or> | <And> | <Not> | <Lesser> | <LessEqual> | <Greater> | <GreatEqual> | <Eq> | <NotEq> | <Add> | <Multiply> | <Divide> | <Subtract> | <Power> | <Modulo> | <Xor> | <Xand> | <LeftShift> | <RightShift> | <UnaryNeg>
<Or> ::= <UnAmb> OR <UnAmb> | <UnAmb> OR <Or>
<And> ::= <UnAmb> AND <UnAmb> | <UnAmb> AND <And>
<Not> ::= NOT <UnAmb> | NOT <Not>
<Lesser> ::= <UnAmb> LE <UnAmb> | <UnAmb> LE <Lesser> | <UnAmb> LE <LessEqual>
<LessEqual> ::= <UnAmb> LEQ <UnAmb> | <UnAmb> LEQ <Lesser> | <UnAmb> LEQ <LessEqual>
<Greater> ::= <UnAmb> GE <UnAmb> | <UnAmb> GE <Greater> | <UnAmb> GE <GreatEqual>
<GreatEqual> ::= <UnAmb> GEQ <UnAmb> | <UnAmb> GEQ <Greater> | <UnAmb> GEQ <GreatEqual>
<Eq> ::= <UnAmb> EQ <UnAmb> | <UnAmb> EQ <Eq>
<NotEq> ::= <UnAmb> NEQ <UnAmb>
<Subtract> ::= <UnAmb> NEG <UnAmb> | <Add> NEG <UnAmb>
<Add> ::= <UnAmb> ADD <UnAmb> | <UnAmb> ADD <Add>
<Divide> ::= <UnAmb> DIV <UnAmb> | <Multiply> DIV <UnAmb>
<Multiply> ::= <UnAmb> MUL <UnAmb> | <UnAmb> MUL <Multiply>
<Power> ::= <UnAmb> POW <UnAmb>
<Modulo> ::= <UnAmb> MOD <UnAmb>
<Xor> ::= <UnAmb> XOR <UnAmb> | <UnAmb> XOR <Xor>
<Xand> ::= <UnAmb> XAND <UnAmb> | <UnAmb> XAND <Xand>
<LeftShift> ::= <Unamb> LSHIFT <Unamb>
<RightShift> ::= <Unamb> RSHIFT <Unamb>
<UnaryNeg> ::= NEG <UnAmb> | NEG <UnaryNeg>
<UnAmb> ::= IDEN | <Number> | <Bool> | LPAREN <Exp> RPAREN | <FunCall>
<Number> ::= DEC | BIN | HEX | OCT
<Bool> ::= TRUE | FALSE
<Type> ::= TBOOL | TI8 | TI16 | TI32 | TI64 | TI128 | TU8 | TU16 | TU32 | TU64 | TU128 | TF8 | TF16 | TF32 | TF64 | TF128 | TCHAR