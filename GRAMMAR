<Prog> ::= <Decl> | <Decl> <Prog>
<Block> ::= LBRACE RBRACE | LBRACE <Prog> RBRACE
<Decl> ::= <VarDecl> | <Stmt> | <FunDecl>
<FunCall> ::= IDEN LPAREN <Params> RPAREN
<FunDecl> ::= FUN <Type> IDEN LPAREN <Params> RPAREN <Block> | FUN <Type> IDEN LPAREN RPAREN <Block>
<Params> ::= <Param> | <Param> COMMA <Params>
<Param> ::= <Type> IDEN
<Stmt> ::= <ExpStmt> | <Block> | <IfStmt> | <LogStmt> | <RetStmt>
<RetStmt> ::= RETURN <ExpStmt>
<IfStmt> ::= IF <Val> <Block> | IF <Val> <Block> <ElseStmt> | IF <Val> <Block> <ElifStmts> | IF <Val> <Block> <ElifStmts> <ElseStmt>
<ElseStmt> ::= ELSE <Block>
<ElifStmts> ::= <ElifStmt> | <ElifStmt> <ElifStmts>
<ElifStmt> ::= ELIF <Val> <Block>
<LogStmt> ::= LOG <Val> SEMC
<ExpStmt> ::= <Val> SEMC
<VarDecl> ::= VAR <Type> IDEN SEMC | VAR <Type> <Assn> SEMC
<Val> ::= <Exp> | <Assn> | NULL
<Assn> ::= IDEN ASSIGN <Val>
<Exp> ::= <UnAmb> | <Or> | <And> | <Not> | <Lesser> | <Greater> | <Eq> | <NotEq> | <Add> | <Multiply> | <Divide> | <Subtract> | <Power> | <Modulo> | <Xor> | <Xand> | <Shift> | <UnaryNeg>
<Or> ::= <UnAmb> | <UnAmb> OR <Or>
<And> ::= <UnAmb> | <UnAmb> AND <And>
<Not> ::= <UnAmb> | NOT <Not>
<Lesser> ::= <UnAmb> LE <UnAmb> | <UnAmb> LEQ <UnAmb> | <UnAmb> LE <Lesser> | <UnAmb> LEQ <Lesser> | <UnAmb> EQ <Lesser>
<Greater> ::= <UnAmb> GE <UnAmb> | <UnAmb> GEQ <UnAmb> | <UnAmb> GE <Greater> | <UnAmb> GEQ <Greater> | <UnAmb> EQ <Greater>
<Eq> ::= <UnAmb> EQ <UnAmb>
<NotEq> ::= <UnAmb> NEQ <UnAmb>
<Add> ::= <UnAmb> | <UnAmb> ADD <Add>
<Multiply> ::= <UnAmb> | <UnAmb> MUL <Multiply>
<Divide> ::= <UnAmb> | <UnAmb> DIV <Divide>
<Subtract> ::= <UnAmb> | <UnAmb> NEG <Subtract>
<Power> ::= <UnAmb> | <UnAmb> POW <Power>
<Modulo> ::= <UnAmb> | <UnAmb> MOD <Modulo>
<Xor> ::= <UnAmb> | <UnAmb> XOR <Xor>
<Xand> ::= <UnAmb> | <UnAmb> XAND <Xand>
<Shift> ::= <Unamb> LSHIFT <Unamb> | <Unamb> RSHIFT <Unamb>
<UnaryNeg> ::= <UnAmb> | NEG <UnAmb> | ADD <UnAmb>
<UnAmb> ::= IDEN | <Number> | BOOL | LPAREN <Exp> RPAREN | <FunCall>
<Number> ::= DEC | BIN | HEX | OCT
<Type> ::= TBOOL | TI8 | TI16 | TI32 | TI64 | TI128 | TU8 | TU16 | TU32 | TU64 | TU128 | TF8 | TF16 | TF32 | TF64 | TF128 | TCHAR